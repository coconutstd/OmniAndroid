<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/textScoreId"
        android:layout_width="1dp"
        android:layout_height="48dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="24dp"
        android:text="데이터 값"
        app:layout_constraintStart_toEndOf="@+id/textTotal_time"
        app:layout_constraintTop_toTopOf="parent"
        android:visibility="invisible"/>

    <TextView
        android:id="@+id/textMeeting_title"
        android:layout_width="100dp"
        android:layout_height="48dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="3dp"
        android:text="제목"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textScoreUserId"
        android:layout_width="90dp"
        android:layout_height="48dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="3dp"
        android:text="이름"
        app:layout_constraintStart_toEndOf="@+id/textMeeting_title"
        app:layout_constraintTop_toTopOf="parent" />



    <TextView
        android:id="@+id/textTotal_time"
        android:layout_width="130dp"
        android:layout_height="48dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="24dp"
        android:text="총 시간"
        app:layout_constraintStart_toEndOf="@+id/textUserId"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textApplied_score"
        android:layout_width="70dp"
        android:layout_height="48dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="24dp"
        android:text="데이터 값"
        app:layout_constraintStart_toEndOf="@+id/textTotal_time"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textScore_per_second"
        android:layout_width="1dp"
        android:layout_height="48dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="24dp"
        android:text="데이터 값"
        app:layout_constraintStart_toEndOf="@+id/textTotal_time"
        app:layout_constraintTop_toTopOf="parent"
        android:visibility="invisible"/>

    <TextView
        android:id="@+id/textScoreCreatedAt"
        android:layout_width="1dp"
        android:layout_height="48dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="24dp"
        android:text="데이터 값"
        app:layout_constraintStart_toEndOf="@+id/textTotal_time"
        app:layout_constraintTop_toTopOf="parent"
        android:visibility="invisible"/>

</androidx.constraintlayout.widget.ConstraintLayout>